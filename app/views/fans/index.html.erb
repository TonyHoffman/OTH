<h1>Listing fans</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Fan</th>
    <th>League</th>
    <th>Password</th>
    <th>Fan</th>
    <th>User name</th>
    <th>Is admin</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @fans.each do |fan| %>
  <tr>
    <td><%= fan.email %></td>
    <td><%= fan.fan %></td>
    <td><%= fan.league_id %></td>
    <td><%= fan.password %></td>
    <td><%= fan.fan_id %></td>
    <td><%= fan.user_name %></td>
    <td><%= fan.is_admin %></td>
    <td><%= link_to 'Show', fan %></td>
    <td><%= link_to 'Edit', edit_fan_path(fan) %></td>
    <td><%= link_to 'Destroy', fan, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Fan', new_fan_path %>
